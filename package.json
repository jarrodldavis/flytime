{
  "name": "@jarrodldavis/flytime",
  "description": "Distributed team timezone tracking",
  "version": "0.0.1",
  "author": "Jarrod Davis <developer@jarrodldavis.com>",
  "homepage": "https://github.com/jarrodldavis/flytime#readme",
  "repository": "jarrodldavis/flytime",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "patch-package",
    "dev": "sapper dev",
    "dev:test": "npm run dev --node-options='-r dotenv/config -r ./mock-redis'",
    "wait": "wait-on http://localhost:3000",
    "build": "sapper build --legacy",
    "lint": "run-s lint:* -cns",
    "lint:es": "eslint . --ext js,svelte",
    "lint:style": "stylelint '**/*.css' '**/*.svelte'",
    "lint:prettier": "prettier '**' --check",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev:test cy:run"
  },
  "engines": {
    "node": "^12.1.0",
    "npm": "^6.9.0"
  },
  "dependencies": {
    "@polka/parse": "^1.0.0-next.3",
    "@polka/redirect": "^1.0.0-next.3",
    "@polka/send": "^1.0.0-next.3",
    "@slack/web-api": "^5.0.1",
    "accepts": "^1.3.7",
    "connect-redis": "^3.4.1",
    "express-session": "^1.16.1",
    "http-errors": "^1.7.2",
    "luxon": "^1.13.2",
    "pino": "^5.12.3",
    "pino-http": "^4.2.0",
    "polka": "^1.0.0-next.3",
    "redis": "^2.8.0",
    "sirv": "^0.4.0",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "acorn-class-fields": "^0.3.1",
    "chokidar": "^3.0.0",
    "cypress": "^3.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-problems": "^2.0.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-svelte3": "^1.1.0",
    "mock-require": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.1.2",
    "pino-pretty": "^3.0.0",
    "prettier": "^1.17.0",
    "prettier-plugin-svelte": "^0.4.2",
    "redis-mock": "^0.43.0",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.26.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "svelte": "^3.2.1",
    "wait-on": "^3.2.0"
  }
}
